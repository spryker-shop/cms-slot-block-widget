{
  "name": "spryker-shop/cms-slot-block-widget",
  "type": "library",
  "description": "CmsSlotBlockWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/shop-cms-slot-extension": "^0.2.0",
    "spryker/cms-slot-block": "^0.1.0",
    "spryker/cms-slot-block-storage": "^0.1.0",
    "spryker/kernel": "^3.30.0",
    "spryker/twig": "^1.0.0 || ^2.0.0 || ^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker-shop/cms-block-widget": "If you want to use CmsBlockTwigPlugin. Minimum required version: 2.0.0."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
